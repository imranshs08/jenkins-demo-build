pipeline {
    agent any 
    
    environment{
        PATH="/usr/share/maven:$PATH"
    }
    stages {
        stage("Git Checkout"){
            steps{
                git 'https://github.com/imranshs08/jenkins-demo-build.git'
            }
        }
        stage("Maven Build"){
            steps{
              sh "mvn clean package"  
            }
        }
    }
}
